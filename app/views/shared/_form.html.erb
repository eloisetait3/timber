
    <%= simple_form_for [@user, @house] do |f| %>
      <%= f.input :name %>
      <%= f.input :location %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden, class: 'form-control'  %>
      <%= f.button :submit, class: 'btn btn-primary' %>
    <% end %>
