<div class = "container">
  <nav class="navbar navbar-light bg-light">
  <span class="navbar-brand mb-0 h1">Navbar</span>
  </nav>
  <p><%= link_to 'Profile', user_path(current_user) %></p>
  <div class = "filter-form">
    <h2>This will be a search filter-form</h2>
  </div>
  <div class = "treehouses-index-display">
  	<ul>
  	  <%@houses.each do |house|%>
      	<li>
      	  <h3><%= link_to house.name, house_path(house) %></h3>
      	  <p><%= house.description %></p>
      	  <p><%= house.price %> | <%= house.location %></p>
      	</li> 
      <%end%>
    </ul>
  </div>
</div>